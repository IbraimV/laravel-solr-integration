{
  "name": "ibraimv/laravel-solr-integration",
  "description": "A Laravel package for seamless Solr integration",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ibraim V",
      "email": "asdsd@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Ibraimv\\SolrIntegration\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ibraimv\\SolrIntegration\\SolrServiceProvider"
      ],
      "aliases": {
        "Solr": "Ibraimv\\SolrIntegration\\Facades\\Solr"
      }
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^11.4",
    "guzzlehttp/psr7": "^2.7",
    "mockery/mockery": "^1.6"
  }
}
